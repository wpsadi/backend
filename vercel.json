{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun db:generate:prod && bun code:build",
  "devCommand": "bun dev:start",
  "installCommand": "bun install",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "build/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}