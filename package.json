{
	"name": "sdc-website-workers-openapi",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "bun --hot run src/index.ts --watch",
		"dev:start": "bun run src/index.ts ",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"db:generate": "NODE_ENV=development drizzle-kit generate",
		"db:migrate": "NODE_ENV=development drizzle-kit migrate",
		"db:push": "NODE_ENV=development drizzle-kit push",
		"db:studio": "NODE_ENV=development drizzle-kit studio",
		"code:build": "bun build ./src/index.ts --outdir ./build --target=bun --sourcemap",
		"start": "NODE_ENV=production bun build/index.js"
	},
	"dependencies": {
		"chanfana": "^2.8.3",
		"dotenv": "^17.2.3",
		"drizzle": "^1.4.0",
		"drizzle-orm": "^0.44.6",
		"hono": "^4.6.20",
		"pg": "^8.16.3",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/bun": "latest",
		"@types/pg": "^8.15.5",
		"@types/service-worker-mock": "^2.0.4",
		"drizzle-kit": "^0.31.5",
		"tsx": "^4.20.6"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"packageManager": "bun@1.3.0"
}